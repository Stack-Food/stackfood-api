{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "coreclr",
      "request": "launch",
      "name": "StackFood.API (Desenvolvimento)",
      "program": "${workspaceFolder}/src/Adapters/Driving/StackFood.API/bin/Debug/net7.0/StackFood.API.dll",
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "launchBrowser": {
        "enabled": true,
        "args": "http://localhost:5000/swagger",
        "windows": {
          "command": "cmd.exe",
          "args": "/C start http://localhost:5000/swagger"
        }
      }
    },
    {
      "type": "coreclr",
      "request": "launch",
      "name": "StackFood.API (Produção)",
      "program": "${workspaceFolder}/src/Adapters/Driving/StackFood.API/bin/Debug/net7.0/StackFood.API.dll",
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Production"
      },
      "launchBrowser": {
        "enabled": true,
        "args": "http://localhost:5000/swagger",
        "windows": {
          "command": "cmd.exe",
          "args": "/C start http://localhost:5000/swagger"
        }
      }
    },
    {
      "name": ".NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/Adapters/Driving/StackFood.Worker/bin/Debug/net8.0/StackFood.Worker.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/Adapters/Driving/StackFood.Worker",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    },
    {
      "name": "Containers: .NET Launch",
      "type": "docker",
      "request": "launch",
      "preLaunchTask": "docker-run: debug",
      "netCore": {
        "appProject": "${workspaceFolder}/src/Adapters/Driving/StackFood.Worker/StackFood.Worker.csproj"
      }
    }
  ]
}
